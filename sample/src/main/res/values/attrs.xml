<resources xmlns:tools="http://schemas.android.com/tools">


    <!--虚线-->
    <attr name="dash_line_color" format="color" tools:ignore="ResourceName" />
    <attr name="dash_line_clearance" format="dimension" tools:ignore="ResourceName" />
    <declare-styleable name="DashLineView" tools:ignore="ResourceName">
        <attr name="dash_line_color" tools:ignore="ResourceName" />
        <attr name="dash_line_clearance" tools:ignore="ResourceName" />
        <attr name="dash_line_orientation" tools:ignore="ResourceName">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>

    <!--横向或者纵向滚动的ViewPage-->

    <declare-styleable name="VerticalOrHorizontalViewPager" tools:ignore="ResourceName">
        <attr name="orientation" tools:ignore="ResourceName">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="FullscreenAttrs">
        <attr name="fullscreenBackgroundColor" format="color" />
        <attr name="fullscreenTextColor" format="color" />
    </declare-styleable>

</resources>
